<div class="maincontent">

    <% if (indexData) { %>
        <% for (let i = 0; i < indexData.length; i++) { %>
            <div class="row mt-1">
                <div class="col-12">
                    <div class="card">
                        <div class="row card-body">
                            <div class="col-3">
                                <img class="logo-img"
                                     src="https://source.unsplash.com/random/800x800/?img=<%= Math.floor(Math.random() * 10000)%>"
                                     alt="sans"/>
                            </div>
                            <div class="col-9">
                                <div class="row">
                                    <div class="col-9">
                                        <h5 class="card-title"><%= indexData[i].title %></h5>
                                    </div>
                                    <div class="col-3">
                                        <small class="float-end"><%= moment(indexData[i].date).format('DD-MMM-YYYY') %></small>
                                    </div>
                                </div>
                                <p class="card-text"><%= indexData[i].company %></p>
                                <p class="card-text"><%= indexData[i].about %></p>
                                <a href="#" class="btn btn-primary float-end">Go somewhere</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        <% } %>
    <% } %>
</div>
